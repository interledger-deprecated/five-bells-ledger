{
  "name": "five-bells-ledger",
  "version": "21.2.1",
  "description": "Five Bells ledger reference implementation",
  "keywords": [
    "interledger",
    "five-bells",
    "ilp"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:interledgerjs/five-bells-ledger.git"
  },
  "scripts": {
    "start": "node src/app.js",
    "start-prof": "node --prof --logfile=${LEDGER_V8_LOGFILE:-v8.log} src/app.js",
    "lint": "eslint .",
    "test": "mocha",
    "coverage": "istanbul cover _mocha",
    "report-coverage": "codecov",
    "docs": "npm run docs:prep-intro && npm run docs:apidoc",
    "docs:prep-intro": "scripts/build_docs.js",
    "docs:apidoc": "apidoc -o apidoc-out -i src/controllers/",
    "integration": "integration-loader && integration all",
    "bump": "version-bump"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "dependencies": {
    "bignumber.js": "^4.1.0",
    "canonical-json": "0.0.4",
    "co": "^4.6.0",
    "co-body": "^5.1.1",
    "co-defer": "^1.0.0",
    "co-emitter": "^0.2.3",
    "deep-diff": "^0.3.8",
    "eslint-plugin-standard": "^3.0.1",
    "extensible-error": "^1.0.2",
    "five-bells-condition": "^5.0.1",
    "five-bells-shared": "^26.1.0",
    "http-errors": "^1.6.2",
    "jsonwebtoken": "^8.1.0",
    "kcors": "^2.2.1",
    "knex": "^0.14.1",
    "koa": "^2.4.1",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-cors": "0.0.16",
    "koa-json-error": "^3.1.2",
    "koa-passport": "^4.0.1",
    "koa-riverpig": "^2.0.0",
    "koa-router": "^7.3.0",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.4",
    "methods": "^1.1.2",
    "moment": "^2.19.2",
    "passport-anonymous": "^1.0.1",
    "passport-client-certificate": "^0.1.1",
    "passport-http": "^0.3.0",
    "passport-http-signature": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "pg-migrator": "^1.0.5",
    "priorityqueuejs": "^1.0.0",
    "promise-retry": "^1.1.1",
    "riverpig": "^1.1.2",
    "spdy": "^3.4.7",
    "tweetnacl": "^1.0.0",
    "ws": "^3.3.2"
  },
  "optionalDependencies": {
    "mysql": "^2.15.0",
    "pg": "^7.4.0",
    "tedious": "^2.1.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "codecov": "^3.0.0",
    "ejs": "^2.5.7",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "five-bells-integration-test-loader": "^1.3.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "~4.0.1",
    "mock-require": "^2.0.2",
    "nock": "^9.1.0",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "spec-xunit-file": "0.0.1-3",
    "sqlite3": "^3.1.13",
    "supertest": "^3.0.0",
    "through2": "^2.0.3"
  },
  "config": {
    "five-bells-integration-test-loader": {
      "module": "five-bells-integration-test",
      "repo": "interledgerjs/five-bells-integration-test"
    }
  }
}
